    "1": {
      "icon": { "displayName": "TextSelect" },
      "type": "Text",
      "label": "Text",
      "textarea": "My goal was to create a video that would transport viewers to the summit of Mt. Everest. No music. No sound effects. No slow motion. No extreme wide angle helmet-cam footage. Just the raw sound of the environment, the flow of oxygen being inhaled through climbers‚Äô masks and the deep struggle and gasps for air as each person endures, suffers and ultimately prevails.",
      "style": {
        "backgroundColor": "",
        "color": "#000000",
        "padding": "10px",
        "textAlign": "left",
        "fontSize": "15px",
        "fontWeight": "normal",
        "textTransform": "lowercase"
      },
      "outerStyle": { "backgroundColor": "#fff", "width": "100%" },
      "id": 1737830296883
    },
